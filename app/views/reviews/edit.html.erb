<h1>Edit Review</h1>

<%= form_for([@post, @review]) do |f| %>
    <div id="rating-form">
      <label>Rating</label>
    </div>
    <%= f.button :submit,class: 'btn btn-primary' %>
<% end %>

<script>
    $('#rating-form').raty({
        path: '/assets/',
        scoreName: 'review[rating]'
    });
</script>